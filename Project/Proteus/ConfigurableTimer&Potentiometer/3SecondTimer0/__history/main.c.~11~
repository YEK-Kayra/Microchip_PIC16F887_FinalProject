#include <16f887.h>

#use delay(clock=4M)
#use fast_io(b) 

#fuses HS,NOWDT,NOPUT,NOLVP,NOCPD,NOPROTECT,NODEBUG,NOBROWNOUT,NOWRT

#define ledPin pin_B0

#INT_TIMER0

void main(void) 
{


 set_tris_b(0x00);               


 while(1) //Sonsuz döngü baþlangýcý
 {
 
output_high(ledPin);
delay_ms(1000);
output_low(ledPin);
delay_ms(1000);
         //TimingNumber zamanlayýcýdan alýnacak

  
 }
} 
